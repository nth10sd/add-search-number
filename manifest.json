{
  "manifest_version": 2,
  "name": "Add Search Number",
  "description": "Add Search Number allows going to search results by pressing a number instead of clicking.",
  "homepage_url": "https://github.com/nth10sd/add-search-number",
  "version": "1.9.0",
  "icons": {
    "32": "icons/icon.png"
  },
  "background": {
      "scripts": [
        "background.js"
      ]
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["addsearchnumber.js"]
    }
  ]
}
